{
  "show_edit_predictions": true,
  "languages": {
    "Rust": {
      "show_edit_predictions": true
    }
  },
  "edit_predictions": {
    "mode": "eager",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_assistant": false
  },
  "assistant": {
    "version": "2",
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "current_line_highlight": "none",
  "cursor_blink": false,
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  },
  "experimental.theme_overrides": {
    "element.active": "#0b0b0b"
  },
  "git": {
    "git_gutter": "hide"
  },
  "git_panel": {
    "button": false
  },
  "gutter": {
    "code_actions": false,
    "folds": false,
    "line_numbers": false
  },
  "indent_guides": {
    "enabled": false
  },
  // "max_tabs": 1,
  "notification_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "project_panel": {
    "button": false,
    "git_status": false
  },
  "scrollbar": {
    "git_diff": false
  },
  "tab_bar": {
    "show": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "button": false
  },
  "theme": "Tokyo Night",
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "vim_mode": true
}
